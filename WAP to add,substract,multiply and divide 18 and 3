#include <stdio.h>

int main() 
{
 
   int num1=18,num2=3,result;
   result=num1+num2;
   printf("\nThe sum of two numbers=%d",result);
    result=num1-num2;
    printf("\nThe difference of two numbers=%d",result);
     result=num1*num2;
     printf("\nThe product of two numbers=%d",result);
      result=num1/num2;
      printf("\nThe division of two numbers=%d",result);
    return 0;
}
